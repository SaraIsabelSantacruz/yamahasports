<%= stylesheet_link_tag 'bootstrap.min'%>
<!-- .main-container close  -->
<div class="main-container">

  <script id="shoe-template" type="x-handlebars-template">​
    {{#each this}}​
      <div class="col-xs-12 col-md-6 {{class}}">
        <div class="shop-item">
          <div class="shop-content" style="margin-bottom: 20px;">
            <div class="shop-image-container uk-inline tw-onhover visible" style="height:280px; width:405px;">
              <img alt="Backpack In Blac" class="own-img" src="{{url}}">
              <div class="uk-position-bottom">
                <a href="/detalle" class="add-to-cart" data-url="{{name}}">Ver Detalle</a>
              </div>
            </div>
            <h4>
              <a href="shop-single.html" class="shop-title">{{name}}</a>
            </h4>
            <a class="shop-category">Accessories</a>
            <div class="price">
              <span class="old-price">{{price}}</span>
              <span class="new-price">{{price}}</span>
            </div>
          </div>
        </div>
      </div>
    {{/each}}
    ​</script>

  <section class="tw-slider tw-slider-fullscreen uk-light own-catalogo">
    <div class="owl-carousel owl-theme">
      <div class="slider-item" data-overlay="0.4" style="background-color: #151515; background-image: url(/demo/shop/shop-bg1.jpg);">
        <div class="tw-page-title-container tw-element">
          <h1 class="tw-page-title uk-text-uppercase uk-text-center uk-flex uk-flex-middle uk-flex-center" style="padding-top:50vh; margin-top: 0px;">Catálogo</h1>
        </div>
      </div>
    </div>
  </section>

  <section class="uk-section uk-section-shop">
    <div class="uk-container">
      <div data-uk-grid>
        <div class="content-area uk-width-expand">
          <div class="tw-element tw-shop">
            <div class="container-fluid" style="padding:0px;">
              <div class="row">
                <div class="cont-templeate"></div>
              </div>
            </div>
          </div>
        </div>

        <div class="sidebar-area">
          <div class="sidebar-inner" data-uk-sticky="bottom: true;offset: 40">
            <div class="widget-item">
              <aside class="widget widget_categories">
                <h3 class="widget-title">
                  <span>Categories</span>
                </h3>
                <ul>
                  <li class="cat-item cat-item-2 current-cat">
                    <a href="catal1">Autmáticas y Semiautomáticas</a>(18)</li>
                  <li class="cat-item cat-item-7">
                    <a href="catal2">Urbanas trabajo y Deportivas</a>(16)</li>
                  <li class="cat-item cat-item-9">
                    <a href="catal3">Todo Terreno</a>
                    (13)</li>
                  <li class="cat-item cat-item-8">
                    <a href="catal4">Deportivas</a>
                    (14)</li>
                  <li class="cat-item cat-item-9">
                    <a href="catal5">Súper Deportivas</a>
                    (13)</li>
                  <li class="cat-item cat-item-8">
                    <a href="catal6">Sport Heritage</a>
                    (14)</li>
                  <li class="cat-item cat-item-2 current-cat">
                    <a href="catal7">Cuatrimotos</a>(18)</li>
                </ul>
              </aside>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>
</div>
<%=javascript_include_tag "catalog"%>
<%=javascript_include_tag "bootstrap.min"%>
